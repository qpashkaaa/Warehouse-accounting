<UserControl x:Class="Warehouse_accounting.View.Components.CustomLogoSidenav"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Warehouse_accounting.View.Components"
             mc:Ignorable="d">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="48"></ColumnDefinition>
            <ColumnDefinition Width="Auto"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Image Grid.Column="0" Source="/Images/LogoIcon.png"  Width="48" Height="50" HorizontalAlignment="Right"></Image>
        <Image Grid.Column="1" Source="/Images/LogoIconName.png" Width="71" Height="28"  HorizontalAlignment="Left" Margin="3 0 0 0"></Image>
    </Grid>
</UserControl>
